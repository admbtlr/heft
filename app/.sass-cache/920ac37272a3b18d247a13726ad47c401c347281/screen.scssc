@linei:@template"‘/* Welcome to Compass.
 * In this file you should write your main styles. (or centralize your imports)
 * Import this file using the following HTML or equivalent:
 * <link href="/stylesheets/screen.css" media="screen, projection" rel="stylesheet" type="text/css" /> */

@import "h5bp";
@import "compass/css3";
@import "compass/css3/transform";

@include h5bp-display;
@include h5bp-selection;
@include h5bp-base;
@include h5bp-links;
@include h5bp-typography;
@include h5bp-lists;
@include h5bp-embeds;
@include h5bp-figures;
@include h5bp-forms;
@include h5bp-tables;

@include h5bp-helpers;


@import "fonts";


@mixin font-size($sizeValue: 1.6, $lineHeight: $sizeValue * 1.4){
    font-size: ($sizeValue * 10) + px;
    line-height: ($lineHeight * 10) + px;
    font-size: $sizeValue + rem;
    line-height: $lineHeight + rem;
}

@mixin iphone {
  @media only screen and (max-width: 480px) {
    @content;
  }
}

* { -moz-box-sizing: border-box; -webkit-box-sizing: border-box; box-sizing: border-box; }

* { -webkit-tap-highlight-color: rgba(0,0,0,0); }

$padding: 50px;
$pageWidth: 480px;
$pageHeight: 690px;

// colours
$editorBg: rgb(230,230,230);
$editorFg: rgb(20,20,20);

// the buttons
.page-buttons a, .edit-buttons a {
    display: inline-block;
    text-decoration: none;
}
.page-buttons, .edit-buttons {
    // border-top: 1px solid #333;
    // opacity: 0.95;
    -webkit-transition: 0.2s;
    width: $pageWidth;
    height: 60px;
    position: absolute;
    bottom: -60px;
    // left: $pageWidth;
    // @include iphone {
    //     left: 0;
    // }
    // margin-top: 0px;
    color: #333;
    padding: 0 15px 10px 0;
    display: none;
    font-size: 50px;
    line-height: 1.3;
    @include transition(0.2s);
    @include transform(translateZ(0px));
}
.page-buttons {
    // @include backface-visibility(hidden);
}
.edit-buttons {
    display: block;
    background-color: $editorBg;
    a {
        color: #333;
    }
    a:hover {
        color: $editorFg;
    }
}
.page-buttons a, .edit-buttons a {
    display: inline-block;
    width: 25%;
    text-align: center;
}

html {
  font-size: 62.5%;
}

html, body {
    height: 100%;
}

.main {
    // postion: absolute;
    width: 100%;
    height: 100%;
    min-height: 100%;
    background: #666 url('/img/cardboard_med.jpg');
    // padding: 50px;
}

.header {
    width: 100%;
    height: 150px;
}

.header > h1 {
    display: none;
    text-transform: lowercase;
    text-shadow: rgba(200,100,100,0.7) 3px -3px 0;
    line-height: 1;
    color: rgba(30,30,0,0.7);
    font-weight: bolder;
    font-size: 100px;
    margin: 0;
    padding: 25px 10px;
    font-family: "HelveticaNeue-Light", "Helvetica Neue Light", "Helvetica Neue", Helvetica, Arial, "Lucida Grande", sans-serif;
}

#search-bar {
    padding: 5px 10px;
    width: 540px;
    position: absolute;
    right: 30px;
    top: 55px;
    height: 40px;
    font-size: 2em;
    border-radius: 20px;
    border: 1px;    
}

#search-bar:focus {
    border: 1px;        
}

.tiles {
    position: absolute;
    left: 0;
    top: 0;
    bottom: 0;
    right: 600px;
    overflow-y: auto;
    // so that we're not scrolling shadows against a fixed bg image...
    background: #666 url('/img/cardboard_med.jpg');

}

.cf {
  display: table;
  clear: both;
}

@media only screen and (max-device-width: 480px) { 
    .active-note, .edit-overlay {
        padding-top: 0px; 
    }
}

body.kill-scrolling {
    overflow: hidden;
}

.active-note {
    position: absolute;
    width: 600px;
    top: 110px;
    bottom: 0;
    right: 0;
    overflow: hidden;
    padding: 30px 20px 20px;
    .buttons {
        background-color: rgba(0,0,0,0.3);
        @include border-radius(0 50px 0 0);
        width: 500px;
        height: 50px;
        margin-top: 50px;
    }
    @include iphone {
        top: 0px;
        padding: 0;
        width: 480px;
    }
}

.edit-pane {
    @include box-shadow(rgba(0,0,0, 0.2) 0 0 10px 0);
    @include border-radius(10px);
    width: 100%;
    height: 100%;
    padding: 20px;
    background-color: $editorBg;
    color: $editorFg;
    border: 0px;
    font: 16px/24px 'source-code-pro';
}
.edit-pane:focus {
    outline: 0;
}

// flippy stuff:
.active-note {
    @include perspective(1000);
}
.flip-holder {
    @include transform-style(preserve-3d);
    @include transition(all 0.5s linear);
    height: 100%;
}
.flip-me {
    @include transform(rotateY(180deg));    
}
.flip-face {
    @include backface-visibility(hidden);
    position: absolute;
    top: 0;
    left: 0;
    margin: 0px;
    width: 100%;
    height: 100%;
}
.active-note-edit {
    @include transform(rotateY(180deg));
}

.tile {
    display: block;
    margin: 1px 1px 0 0;
    width: 150px;
    float: left;
    position: relative;
}

.tile-spinner {
    @include transform(rotateZ(360deg)); 
}

.tile > h1 {
    display: none;
    float: left;
    height: 30px;
    text-shadow: rgba(255,255,255,0.3) 0 1px 0;
    font-size: 1.25em;
    padding: 2px;
    color: rgb(30,30,0);
    margin: 0;
}

.tile .modify-date {
    border-radius: 5px 0 5px 0;
    font-weight: normal;
    right: 0px;
    background-color: rgba(100,100,100, 0.9);
    bottom: 0px;
    position: absolute;
    font-size: 1.25em;
    padding: 2px;
    color: rgb(200,200,200);
    margin: 0;
}

.tiles .selected-note {
    // margin: 10px 5px 0;
    // box-sizing: content-box;
    // border: 10px solid rgb(200,100,100);
    // @include box-shadow(rgb(200,100,100) 0 0 5px);
    // @include border-radius(5px);
    @include transition(0.4s ease-out);
    position: relative;
    z-index: 5;
}

.porthole {
    padding: 0;
    padding: 0;
    // border: 1px solid #999;
    width: 150px;
    height: 150px;
    float: left;
    overflow: hidden;
    // background-color: #ccd;
    text-decoration: none;
    color: inherit;
    // @include box-shadow(rgba(0,0,0,0.2) 0 0 5px 0);
    @include border-radius(5px);

}

/* higher specificity, to override .porthole styles for the selected note */
.active-note-display {
    @include box-shadow(rgba(0,0,0, 0.2) 0 0 10px 0);
    @include border-radius(10px);
    border: 1px solid #999;
}

.active-note-display .note {
    @include transform(scale(1));
    @include backface-visibility(hidden);
    overflow-y: scroll;
    @include border-radius(10px);
    width: 100%;
    height: 100%;
}

.porthole .note {
    -webkit-transform: scale(0.5);
    -webkit-transform-origin: -10% -10%;
    -webkit-transition: -webkit-transform .5s;
    overflow: hidden;
}

.note {
    box-sizing: border-box;
    // margin-top: -40%;
    // margin-left: -20%;
    width: $pageWidth;
    height: $pageHeight;
    margin: 0 auto;
    overflow: scroll;
    // @include border-radius(0 20px 20px 0);

    p {
        -webkit-hyphens: auto;
    }

    @include iphone {
        -webkit-overflow-scrolling: touch;
        -webkit-transform: translateZ(0px);
    }
}

.note p {
    padding: 0;
    margin: 0;
}

.note h1:first-of-type {
    margin-top: 0px;
}

.first-line-bold p:first-child:first-line {
    font-weight: bold;
}

.first-line-larger p:first-child:first-line {
    font-size: 180%;
}


// page turning stuff for the notebooks...

#book-holder {
   @include perspective(2000);
}

.book {
    position: relative;
    width: $pageWidth*2;
    height: $pageHeight;
    @include iphone {
        left: -480px;
    }
}

.page {
    @include transform-style(preserve-3d);
    @include transition(transform 0.5s ease-in-out);
    @include transform(translate3d(0px, 0px, 0px));
    -webkit-user-select: none;
    -moz-user-select: -moz-none;
    -ms-user-select: none;
    user-select: none;
}

// .page-holder
.side {
    box-sizing: border-box;
    position: absolute;
    overflow: hidden; // for the buttons...
    top: 0px;
    left: $pageWidth;
    width: $pageWidth;
    height: $pageHeight;
    @include transition(background-color 0.5s linear);
    @include backface-visibility(hidden);
}

.verso {
    @include transform(rotateY(180deg));    
    background-color: #ffe;
}

.recto {
    background-color: #ffe;
}

.flipped {
    @include transform(rotateY(-180deg));    
}

#page-store {
    display: none;
}
